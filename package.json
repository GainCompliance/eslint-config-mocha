{
  "name": "@gaincompliance/eslint-config-mocha",
  "description": "Shareable config for ESLint for Mocha tests",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "GainCompliance/eslint-config-mocha",
  "bugs": "https://github.com/GainCompliance/eslint-config-mocha/issues",
  "homepage": "https://npm.im/@gaincompliance/eslint-config-mocha",
  "author": "Gain Compliance (https://github.com/GainCompliance)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@gaincompliance/eslint-config": "1.0.32",
    "@gaincompliance/remark-preset-lint": "^1.0.2",
    "ban-sensitive-files": "^1.9.2",
    "commitlint-config-gain": "^1.0.17",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "remark-cli": "^9.0.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "eslint-plugin-mocha": "8.0.0"
  }
}
